{
    "java.configuration.updateBuildConfiguration": "automatic",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.dart_tool": true,
        "**/.idea": true,
        "**/build": false,
        "**/.pub-cache/": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.dart_tool": true,
        "**/pubspec.lock": true,
        "**/.pub-cache/": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
    },
    "githubIssues.queries": [
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        },
        {
            "label": "Feedback Issues",
            "query": "state:open repo:${owner}/afrikaburn-app-feedback sort:updated-desc"
        },

    ]
}